#include <stdio.h>
#define MAX 5

int queue[MAX];
int front = -1, rear = -1;

int main()
{
    int n, value;

    printf("Enter number of elements (max %d): ", MAX);
    scanf("%d", &n);

    if (n > MAX)
    {
        printf("Queue Overflow\n");
        return 0;
    }

    front = 0;

    for (int i = 0; i < n; i++)
    {
        scanf("%d", &value);
        queue[++rear] = value;
    }

    printf("Queue elements:\n");
    for (int i = front; i <= rear; i++)
    {
        printf("%d ", queue[i]);
    }

    return 0;
}
